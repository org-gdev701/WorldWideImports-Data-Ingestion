{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "az-env-dev"
		},
		"AKV_AzKVaults_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AzKVaults.vault.azure.net/"
		},
		"SelfHostedIRAzureVm_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/1ae888e8-f242-4f1a-a97b-7cb12c70b281/resourcegroups/wsfdep/providers/Microsoft.DataFactory/factories/az-env-prod/integrationruntimes/SelfHostedIRAzVm"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "az-env-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AKV_AzKVaults_properties_typeProperties_baseUrl": {
						"value": "[parameters('AKV_AzKVaults_properties_typeProperties_baseUrl')]"
					},
					"SelfHostedIRAzureVm_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('SelfHostedIRAzureVm_properties_typeProperties_linkedInfo_resourceId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}